{
  "name": "virtuin-cli",
  "version": "0.0.24",
  "description": "Virtuin Task Dispatcher is used to dispatch tasks.",
  "main": "./distribution/index.js",
  "bin": {
    "virttaskcli": "./distribution/cli.js"
  },
  "scripts": {
    "flow": "flow check",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "build": "./node_modules/.bin/babel ./src --out-dir ./distribution --source-maps",
    "flow:build": "flow-remove-types ./src/ -d ./distribution --all --pretty",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src distribution",
    "prepublish": "yarn run build && yarn run api",
    "start": "./distribution/cli.js",
    "precommit": "lint-staged",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix"
  },
  "files": [
    "distribution/"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail  --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/samteccmd/virtuintaskdispatcher.git"
  },
  "keywords": [
    "Virtuin"
  ],
  "author": "Pages",
  "license": "MIT",
  "homepage": "https://bitbucket.org/samteccmd/virtuintaskdispatcher#readme",
  "dependencies": {
    "@samtec-ash/virtuinlogging": "0.9.15",
    "amqplib": "^0.5.1",
    "babel-runtime": "^6.26.0",
    "commander": "^2.16.0",
    "deep-diff": "^1.0.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "js-yaml": "^3.12.0",
    "uuid": "^3.3.2",
    "virtuin-rest-service": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-optional-chaining": "^7.0.0-beta.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "documentation": "^8.1.2",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-flowtype-errors": "^3.3.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "^0.79.1",
    "flow-copy-source": "^2.0.2",
    "flow-remove-types": "^1.2.3",
    "flow-typed": "^2.3.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.1"
  }
}
