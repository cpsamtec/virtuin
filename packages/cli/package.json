{
  "name": "virtuin-cli",
  "version": "0.4.2",
  "description": "Virtuin Task Dispatcher is used to dispatch tasks.",
  "main": "./distribution/index.js",
  "bin": {
    "virttaskcli": "./distribution/cli.js"
  },
  "scripts": {
    "flow": "flow check",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "prepare": "yarn run build && yarn run build:flow",
    "build": "./node_modules/.bin/babel ./src --out-dir ./distribution --source-maps",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src distribution",
    "start": "node ./distribution/cli.js",
    "precommit": "lint-staged",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "yarn run lint -- --fix"
  },
  "files": [
    "distribution/"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail  --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/samteccmd/virtuintaskdispatcher.git"
  },
  "keywords": [
    "Virtuin"
  ],
  "author": "Pages",
  "license": "MIT",
  "homepage": "https://bitbucket.org/samteccmd/virtuintaskdispatcher#readme",
  "dependencies": {
    "commander": "^2.19.0",
    "dateformat": "^3.0.3",
    "deep-diff": "^1.0.2",
    "dotenv": "^7.0.0",
    "fs-extra": "^7.0.1",
    "ospath": "^1.2.2",
    "virtuintaskdispatcher": "^0.6.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "documentation": "^9.3.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-compat": "^3.0.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-flowtype-errors": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.94.0",
    "flow-copy-source": "^2.0.3",
    "flow-remove-types": "^1.2.3",
    "flow-typed": "^2.5.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5"
  }
}
